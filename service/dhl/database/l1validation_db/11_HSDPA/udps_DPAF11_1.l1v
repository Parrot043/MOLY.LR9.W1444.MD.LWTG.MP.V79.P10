    kal_uint16          OVSFdpch_rl2;   // for SHO delay  
    //kal_uint16        cells_tm;   // for SHO delay only
    //HSDPA parameters
    kal_uint8           num_of_hsscch;
    kal_uint8           harq_preamble_mode;
