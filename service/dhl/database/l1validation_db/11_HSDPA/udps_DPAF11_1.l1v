10562~10838
@10700

[psc_bts1] "PSC of Serving Cell 1"
0~511
@10

[uarfcn_bts2] "UARFCN of Serving Cell 2"
10562~10838
@10600

[psc_bts2] "PSC of Serving Cell 2"
0~511
@511

