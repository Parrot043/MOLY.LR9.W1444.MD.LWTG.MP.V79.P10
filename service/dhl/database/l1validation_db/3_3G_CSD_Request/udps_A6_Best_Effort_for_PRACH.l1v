* For RACH 
******************************************/
[available_SF] "(RACH)Min allowed SF for"
@SF32  32
SF64  64
SF128 128
SF256 256

[preamble_sc] "(RACH) Preamble scrambling code"
0~15
@5

[sig_index] "(RACH) Available signature. Bit string (16)"
0~65535
@65535

[sub_CH_index] "(RACH) Available sub-channel. Bit string (12)"
0~4095
@4095

