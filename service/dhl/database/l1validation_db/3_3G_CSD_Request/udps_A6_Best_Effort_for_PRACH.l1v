-22~5
@-7

[aich_timing] "(RACH) AICH Tx timing, i.e. Tau_p_a"
@slots_3  0
slots_5  1

/******************************************
* For FACH 
******************************************/
[Ts_ccpch] "Timing offset between S-CCPCH and CPICH [x256 chips]"
0~149
@0

[OVSFs_ccpch] "The OVSF code number of the S-CCPCH (0~SF-1)"
