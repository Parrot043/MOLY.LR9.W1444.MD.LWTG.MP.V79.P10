/******************************************************************************
* Data Structure accomnying the above primitive
typedef struct
{
    kal_uint8       ref_count;
    kal_uint16      msg_len;
    
    kal_uint16       uarfcn_bts1;
    kal_uint16       psc_bts1;
    kal_uint16          uarfcn_bts2;
    kal_uint16         psc_bts2;     
 
    kal_uint8       sccpch_slot_format;   
