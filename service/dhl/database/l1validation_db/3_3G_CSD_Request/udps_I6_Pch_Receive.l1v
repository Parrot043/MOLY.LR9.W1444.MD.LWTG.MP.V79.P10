    kal_uint16       OVSFpich;       
    
    kal_int8	    cpich_tx_power;     	
    kal_int8        pich_power_off;     
    kal_uint8       DRX_cycle_length;   
    kal_uint8       PI_num;             
    kal_uint8       page_occa;          
    kal_uint32      DRX_index;          
    kal_bool        sttd_ind;    
    
